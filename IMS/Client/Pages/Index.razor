@layout LoginLayout

@page "/crag"


@{
    if (x == 0)
    {
        <Vids y="UpdateXfromVids" />
    }
    else if (x == 1)
    {
        <ImageDisplay y="UpdateXfromImg" />
    }
    else if (x == 2)
    {
        <OverAllRanking y="UpdateXfromMedal" istimer="true" />
    }
}

